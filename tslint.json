{
  "extends": ["tslint-react", /*"tslint:recommended",*/ "tslint-config-prettier"],
  "rules": {
    "jsx-no-lambda": false,
    "max-line-length": [ true, 120 ],
    "no-console": [
      false,
      "log",
      "error",
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ]

  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
